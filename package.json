{
  "name": "uri-parse-lib",
  "version": "2.5.0",
  "authors": [
    "Yuriy  Panarin <tduble94@gmail.com>"
  ],
  "description": "Small library for parsing URL.",
  "scripts": {
    "build": "rm -rf dist && rollup -c",
    "test": "jest"
  },
  "main": "uri-parse-lib.umd.js",
  "module": "uri-parse-lib.es5.js",
  "es2015": "uri-parse-lib.es2015.js",
  "types": "index.d.ts",
  "typings": "index.d.ts",
  "browser": "uri-parse-lib.web.js",
  "keywords": [
    "url",
    "uri",
    "url parsing",
    "library",
    "query param",
    "uri library",
    "query",
    "parsing",
    "query parsing"
  ],
  "license": "MIT",
  "homepage": "https://github.com/PxyUp/uri-parse-lib",
  "bugs": "https://github.com/PxyUp/uri-parse-lib/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/PxyUp/uri-parse-lib.git"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/lodash-es": "^4.17.5",
    "jest": "^27.2.4",
    "rollup": "^2.57.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "^lodash-es$": "lodash"
    },
    "testEnvironment": "node",
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!lodash-es)"
    ]
  },
  "dependencies": {
    "lodash-es": "^4.17.21"
  }
}
